{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true
        }
    },
    "rules": {
        "no-console": "off",
        "strict": "off",
        "no-underscore-dangle": "off"
    },
    "parser": "babel-eslint"
}
